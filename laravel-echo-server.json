{
	"authHost": "http://localhost:8000",
	"authEndpoint": "/broadcasting/auth",
	"clients": [
		{
			"appId": "zonix-eats-app",
			"key": "zonix-eats-key"
		}
	],
	"database": "redis",
	"databaseConfig": {
		"redis": {
			"host": "127.0.0.1",
			"port": 6379
		},
		"sqlite": {
			"databasePath": "/database/laravel-echo-server.sqlite"
		}
	},
	"devMode": true,
	"host": "0.0.0.0",
	"port": "6001",
	"protocol": "http",
	"socketio": {
		"cors": {
			"origin": "*",
			"methods": ["GET", "POST"]
		}
	},
	"secureOptions": 67108864,
	"sslCertPath": "",
	"sslKeyPath": "",
	"sslCertChainPath": "",
	"sslPassphrase": "",
	"subscribers": {
		"http": true,
		"redis": true
	},
	"apiOriginAllow": {
		"allowCors": true,
		"allowOrigin": "*",
		"allowMethods": "GET, POST, PUT, DELETE, OPTIONS",
		"allowHeaders": "Origin, Content-Type, Accept, Authorization, X-Requested-With"
	}
}